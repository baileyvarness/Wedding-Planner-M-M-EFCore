@model Wedding

<h1>Plan Your Wedding</h1>
<a href="/Dashboard">Dashboard</a>
<a href="/logout">Log Out</a>


<form asp-action="NewWeddingCreated" asp-controller="Home" method="post">
    <div class="form-group">
        <span class="text-danger font-weight-bold" asp-validation-for="WedderOne"></span>
        <label asp-for="WedderOne"></label>
        <input class="form-control" asp-for="WedderOne">
    </div>
    <div class="form-group">
        <span class="text-danger font-weight-bold" asp-validation-for="WedderTwo"></span>
        <label asp-for="WedderTwo"></label>
        <input class="form-control" asp-for="WedderTwo">
    </div>
    <div class="form-group">
        <span class="text-danger font-weight-bold" asp-validation-for="Date"></span>
        <label asp-for="Date"></label>
        <input class="form-control" type="Date" asp-for="Date">
    </div>
    <div class="form-group">
        <span class="text-danger font-weight-bold" asp-validation-for="Address"></span>
        <label asp-for="Address"></label>
        <input class="form-control" asp-for="Address">
    </div>
    
    <button class="btn btn-info">Create</button>
</form>